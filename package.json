{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "NODE_ENV=production pnpm run generate && NODE_ENV=production tsc && tsc-alias",
    "start:dist": "NODE_ENV=production node build/core/main.js",
    "start:dev": "NODE_ENV=development tsx watch src/core/main.ts",
    "start:prod": "NODE_ENV=production tsx watch src/core/main.ts",
    "test": "dotenv -e .env.test prisma db push && dotenv -e .env.test vitest",
    "test:watch": "dotenv -e .env.test prisma db push && dotenv -e .env.test --watch",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma db push",
    "debug": "NODE_ENV=development tsx watch --inspect-brk src/core/main.ts",
    "prepare": "husky",
    "generate": "dotenv -- tsx script/di-autoload.generator.ts && dotenv -- tsx script/controller.generator.ts",
    "format": "prettier --write \"{src,utils,middlewares,script,app}/**/*.ts\"",
    "lint": "eslint \"{src,utils,script,app}/**/*.ts\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts,json,md}": "pnpm format"
  },
  "dependencies": {
    "@fastify/auth": "^5.0.1",
    "@fastify/awilix": "^6.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/passport": "^3.0.1",
    "@fastify/secure-session": "^8.1.0",
    "@fastify/static": "^8.0.4",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@keyv/redis": "^4.1.0",
    "apple-auth": "^1.0.9",
    "argon2": "^0.41.1",
    "awilix": "^12.0.4",
    "cache-manager": "^6.3.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "file-type": "^20.0.1",
    "google-auth-library": "^9.15.0",
    "i18next": "^24.1.0",
    "keyv": "^5.2.2",
    "kysely": "^0.27.5",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "prisma-kysely": "^1.8.0",
    "tsc-alias": "1.8.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "pino-pretty": "^13.0.0",
    "prettier": "3.4.2",
    "prisma": "^6.2.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
